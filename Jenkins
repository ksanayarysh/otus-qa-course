	agent {
		docker {image "docker_otus"}
	}
	stages {
        	stage('Build') {
            		steps {
                		wget https://chromedriver.storage.googleapis.com/75.0.3770.90/chromedriver_linux64.zip
            		}
        	}
        	stage('Test') {
            		steps {
                		echo 'Testing..'
            		}
        	}
        	stage('Deploy') {
            		steps {
                		echo 'Deploying....'
            		}
        	}
    	}
}
